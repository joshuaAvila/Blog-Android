<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyecto_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconoblogapp"
        android:label="@string/nombre_blog"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Proyectoapp"
        android:usesCleartextTraffic="true">
        <activity android:name=".EditarUserInfoActivity"></activity>
        <activity android:name=".CommentActivity" />
        <activity android:name=".EditarPostActivity" />
        <activity android:name=".AddPostActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".OnBoardActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>